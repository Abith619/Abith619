<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_users_form_inherit_custom_event" model="ir.ui.view">
            <field name="name">res.users.form.inherit.custom.event</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='signup_type']" position="after">
                    <field name="x_signup_ip"/>
                    <field name="create_date"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
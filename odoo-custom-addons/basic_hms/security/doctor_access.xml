<?xml version="1.0" encoding="utf-8"?>
<odoo>

 <record model="ir.module.category" id="module_access_ebook_id">
        <field name="name">Ebook Aceess</field>
        <field name="description"> Ebook </field>
    </record>
    
   <record id="group_ebook_all" model="res.groups">
        <field name="name">ALL</field>
        <field name="category_id" ref="module_access_ebook_id" />
    </record>

    <record id="ebook_pat_admin" model="ir.rule">
        <field name="name">admin Ebook</field>
        <field name="model_id" ref="model_medical_doctor"/>
        <field name="domain_force"></field>
        <field name="groups" eval="[(4, ref('basic_hms.group_ebook_all'))]"/>
    </record>

     <record id="group_ebook_pat" model="res.groups">
        <field name="name">ONE USER</field>
        <field name="category_id" ref="module_access_ebook_id" />
    </record>

    <record id="ebook_pat_doc" model="ir.rule">
        <field name="name">Own Ebook</field>
        <field name="model_id" ref="model_medical_doctor"/>
        <field name="domain_force">[('doctor','=',user.partner_id.id)]</field>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_create" eval="True"/>
        <field name="perm_unlink" eval="True"/>
        <field name="groups" eval="[(4, ref('basic_hms.group_ebook_pat'))]"/>
    </record>
   
</odoo>
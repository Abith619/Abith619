<?xml version="1.0" encoding="utf-8"?>
<odoo>

 <record model="ir.module.category" id="module_access_reg_id">
        <field name="name">Reception Aceess</field>
        <field name="description"> Ebook </field>
    </record>
    
   <record id="group_reg_all" model="res.groups">
        <field name="name">ALL</field>
        <field name="category_id" ref="module_access_reg_id" />
    </record>

    <record id="reception_pat_admin" model="ir.rule">
        <field name="name">admin Reception</field>
        <field name="model_id" ref="model_medical_patient"/>
        <field name="domain_force"></field>
        <field name="groups" eval="[(4, ref('basic_hms.group_reg_all'))]"/>
    </record>

     <record id="group_reg_pat" model="res.groups">
        <field name="name">ONE USER</field>
        <field name="category_id" ref="module_access_reg_id" />
    </record>

    <record id="reception_pat_doc" model="ir.rule">
        <field name="name">Own Reception</field>
        <field name="model_id" ref="model_medical_patient"/>
        <field name="groups" eval="[(4, ref('basic_hms.group_reg_pat'))]"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
        <field name="domain_force">['|',('appoinment_by','=',user.partner_id.id),('appoinment_by','=',False)]</field>
    </record>

      
</odoo>
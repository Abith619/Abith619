<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.module.category" id="Calender_overview_module">
        <field name="name">Calender Access</field>
        <field name="description"></field>
    </record>

    <record id="junior_developer_group" model="res.groups">
        <field name="name">Project Coordinator</field>
        <field name="category_id" ref="Calender_overview_module"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="senior_developer_group" model="res.groups">
        <field name="name">Projects Employee</field>
        <field name="category_id" ref="Calender_overview_module"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>   
    </record>

    <record id="hrmodel.menu_hr_leave_trees_settings" model="ir.ui.menu">
        <field name="groups_id" eval="[(4, ref('hrmodel.senior_developer_group'))]"/>
    </record>

</odoo>

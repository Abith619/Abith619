        <field name="inherite_id">sale.sale_order_line_view_form_readonly</field>
        <field name="arch" type="xml">
            <form string="saleorderline">
                    <group>
                      <xpath expr="//field[@name='product_uom_qty']" position="replace">
                        <field name="product_uom_qty"/>
